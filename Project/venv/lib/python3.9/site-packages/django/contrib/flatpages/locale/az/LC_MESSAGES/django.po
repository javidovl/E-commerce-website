#: venv/lib/python3.9/site-packages/django/contrib/flatpages/admin.py:12
msgid "Advanced options"
msgstr "Əlavə imkanlar"

#: venv/lib/python3.9/site-packages/django/contrib/flatpages/apps.py:8
msgid "Flat Pages"
msgstr "Flat Səhifələr"

#: venv/lib/python3.9/site-packages/django/contrib/flatpages/forms.py:10
#: venv/lib/python3.9/site-packages/django/contrib/flatpages/models.py:9
msgid "URL"
msgstr "URL"

#: venv/lib/python3.9/site-packages/django/contrib/flatpages/forms.py:13
msgid ""
"Example: “/about/contact/”. Make sure to have leading and trailing slashes."
msgstr ""
"Məsələn, “/about/contact/”. Əvvəldə və sondakı kəsr xəttinin olmasına diqqət "
"edin."

#: venv/lib/python3.9/site-packages/django/contrib/flatpages/forms.py:16
msgid ""
"This value must contain only letters, numbers, dots, underscores, dashes, "
"slashes or tildes."
msgstr ""
"Burada yalnız hərf, rəqəm, nöqtə, altdan xətt, defis, kəsr xətti və ya "
"tildadan istifadə etmək olar."

#: venv/lib/python3.9/site-packages/django/contrib/flatpages/forms.py:30
msgid "Example: “/about/contact”. Make sure to have a leading slash."
msgstr ""
"Məsələn, “/about/contact”. Əvvəldəki kəsr xəttinin olmasına diqqət edin."

#: venv/lib/python3.9/site-packages/django/contrib/flatpages/forms.py:43
msgid "URL is missing a leading slash."
msgstr "Ünvan başlanğıcında çəp xətt əksikdir."

#: venv/lib/python3.9/site-packages/django/contrib/flatpages/forms.py:48
msgid "URL is missing a trailing slash."
msgstr "Ünvan sonunda çəp xətt əksikdir."

#: venv/lib/python3.9/site-packages/django/contrib/flatpages/forms.py:65
#, python-format
msgid "Flatpage with url %(url)s already exists for site %(site)s"
msgstr "%(site)s saytı üçün artıq %(url)s ünvanlı Flatpage mövcuddur"

#: venv/lib/python3.9/site-packages/django/contrib/flatpages/models.py:10
msgid "title"
msgstr "başlıq"

#: venv/lib/python3.9/site-packages/django/contrib/flatpages/models.py:11
msgid "content"
msgstr "məzmun"

#: venv/lib/python3.9/site-packages/django/contrib/flatpages/models.py:12
msgid "enable comments"
msgstr "şərhlər olsun"

#: venv/lib/python3.9/site-packages/django/contrib/flatpages/models.py:14
msgid "template name"
msgstr "şablonun adı"

#: venv/lib/python3.9/site-packages/django/contrib/flatpages/models.py:18
msgid ""
"Example: “flatpages/contact_page.html”. If this isn’t provided, the system "
"will use “flatpages/default.html”."
msgstr ""
"Məsələn, “flatpages/contact_page.html”. Əgər təchiz edilməsə, sistem "
"“flatpages/default.html” işlədəcək."

#: venv/lib/python3.9/site-packages/django/contrib/flatpages/models.py:23
msgid "registration required"
msgstr "ancaq qeydiyyatlılar üçün"

#: venv/lib/python3.9/site-packages/django/contrib/flatpages/models.py:24
msgid "If this is checked, only logged-in users will be able to view the page."
msgstr ""
"Əgər bura quş qoysanız, ancaq qeydiyyatdan keçib sayta daxil olmuş "
"istifadəçilər bu səhifəni görə biləcəklər."

#: venv/lib/python3.9/site-packages/django/contrib/flatpages/models.py:27
msgid "sites"
msgstr "saytlar"

#: venv/lib/python3.9/site-packages/django/contrib/flatpages/models.py:31
msgid "flat page"
msgstr "adi səhifə"

#: venv/lib/python3.9/site-packages/django/contrib/flatpages/models.py:32
msgid "flat pages"
msgstr "adi səhifələr"
